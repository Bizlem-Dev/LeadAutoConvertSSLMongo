 <script type="text/javascript">
 var slingip = localStorage.getItem('slingip');
    $('.faq li .question').click(function () {
      $(this).find('.plus-minus-toggle').toggleClass('collapsed');
      $(this).parent().toggleClass('active');
    });

    $('.start-btn').click(function () {
      $('.start-btn-custom-green').removeAttr('disabled');
	  var selected = [];
     $("select[name^='ContactSource']").children('option:selected').each( function() {
        var $this = $(this);
        selected.push( { text: $this.text(), value: $this.val() } );
    });
	  var productArray=[];
	  
      var ContactName = document.getElementsByName("ContactName");
	  var ContactEmail = document.getElementsByName("ContactEmail");
	  var ContactLocation = document.getElementsByName("ContactLocation");
	  var ContactCompany = document.getElementsByName("ContactCompany");
	  var ContactDesignation = document.getElementsByName("ContactDesignation");
        for(var i = 0; i < ContactName.length; i++) {
		 console.log('Item added '+selected[i].value);
		 if(ContactEmail[i].value != ''){
         var dataSource={"FirstName":ContactName[i].value,"LastName":ContactName[i].value,"PhoneNumber":"blank", "Email":ContactEmail[i].value, "Country":ContactLocation[i].value,"CompanyHeadCount":ContactDesignation[i].value,"CompanyName":ContactCompany[i].value,"Source":selected[i].value,"Industry":"freeTrialFormCampaign","Institute": "loggedInuser","SessionCount":0,"Recent_SessionCount":0,"AvgSesionDuration":0};
	
		 var productJSON={"type":"freeTrialFormCampaign","createdBy":"loggedInuser","name": "loggedInuser", "firstname":ContactName[i].value, "emailid":ContactEmail[i].value, "location":ContactLocation[i].value,"designation":ContactDesignation[i].value,"companyname":ContactCompany[i].value,"source":selected[i].value};
         $.ajax({ 
                          url: 'https://'+slingip+':8088/apirest/wsapi/postWpapi/',
                          type: 'POST', 
                          async: true,						  
                         // dataType: 'json',  
                          data: JSON.stringify(productJSON),
						  headers: {
                          'Accept': 'application/json',
                          'Content-Type': 'application/json'
                          },
                          success: function (data, textStatus, xhr) {
						  console.log('campaign set successfully for free trial');
						  },
						  error: function (xhr, textStatus, errorThrown) {  
                         console.log('Error while setting funnel'+errorThrown);  
                     }
						 });
		 productArray.push(dataSource);
		 }
		 }
		 var mailTemplate={"MailTempName":"fixedFreetrialTemplate","templateName":"","typeDataSource":"Enter manually","AttachtempalteType":"","esignature":"false","twofactor":"false","esigntype":"","Email":"jobs@bizlem.com","group":"DoctigerMailTemplate","lgtype":"null","multipeDropDown":["1","3"],"Type":"Generation","data":productArray};
		 $.ajax({ 
                          url: 'https://'+slingip+':8083/portal/servlet/service/dDependencySKU1',
                          type: 'POST', 
                          async: true,						  
                         // dataType: 'json',  
                          data: JSON.stringify(mailTemplate),
						  headers: {
                          'Accept': 'application/json',
                          'Content-Type': 'application/json'
                          },
                          success: function (data, textStatus, xhr) {
						  console.log('mail sent successfully for free trial');
						  },
						  error: function (xhr, textStatus, errorThrown) {  
                         console.log('Error while sending mail'+errorThrown);  
                     }
						 });
		 
		 	 
		 console.log('mailTemplate created '+JSON.stringify(mailTemplate));
    });
    $('#sendQuote').click(function () {
      $('#sendQuoteDiv').css('display','block');
    });
    $('#viewRules').click(function () {
      $('#sendQuoteDiv').css('display','none');
    });

    $('.card-deck a').fancybox({
      caption : function( instance, item ) {
        return $(this).parent().find('.card-text').html();
      }
    });

  </script>
