	<script>
$(document).ready(function(){

    $("#listname").blur(function(){
       if( $("#listname").val().length > 4){
        var value = this.value;
    	$.ajax({
            type : 'POST',
            url :'<%=request.getContextPath()%>/servlet/service/checkingavailabledata.data',
           data: {
        	   listnameresult : value
                 },
			success: function (dataa) {
				if(dataa == "true"){
					document.getElementById("listname").value = "";
					document.getElementById("savefirstpage").disabled = true;
					document.getElementById("listspan").style.display = "";
				}else {
					document.getElementById("savefirstpage").disabled = false;
				}
			}
       });
       }
    });
});
</script>
	<script>
$( document).ready(function() {
    var remoteuser ='<%=request.getRemoteUser()%>';
	$.ajax({
        type : 'POST',
        url :'<%=request.getContextPath()%>/servlet/service/LEAD_CONVERTER_NodeAdd_Email.userdata',
       data: {
    	   remoteuser : remoteuser,
             },
		success: function (dataa) {
		//	alert	(dataa);
			var jsonData = JSON.parse(dataa);
			var remoteuser=jsonData.remoteuser;
			var explore = "";
			var inform="";
			var warm="";
			var entice="";
			var convert="";
		//	alert(remoteuser);
			for (var i = 0; i < jsonData.data.length; i++) {
			    var data = jsonData.data[i];
			    var subject=data.Subject;
			  //  alert(subject);
			    var type=data.type;
			  //  alert(type);
			  //exploreDyna
			  if(type == "Explore"){
				 // '<a style=cursor:pointer onclick=OpenChatBox("'+array.List_id+'","'+array.List_Name+'")>'+array.List_Name+'</a><br>'
				  explore = explore + '<div  style="cursor:pointer;width:150px;overflow:hidden;border:1px solid #000" onclick=move("'+type+'","'+data.Nodes+'","editor1");>'+subject+'</div> <br>';
			   
			  }
			  if(type == "Inform" ){
				  inform = inform + '<div  style="cursor:pointer;width:150px; overflow:hidden; border:1px solid #000" onclick=move("'+type+'","'+data.Nodes+'","editor2");>'+subject+'</div> <br>';
			   }
			  if(type == "Warm"){
				  warm = warm + '<div  style="cursor:pointer;width:150px; overflow:hidden; border:1px solid #000" onclick=move("'+type+'","'+data.Nodes+'","editor3");>'+subject+'</div> <br>';
			   }
			  if(type == "Entice"){
				  entice = entice + '<div  style="cursor:pointer;width:150px; overflow:hidden; border:1px solid #000" onclick=move("'+type+'","'+data.Nodes+'","editor4");>'+subject+'</div> <br>';
			   }
			  if(type == "Convert"){
				  convert = convert + '<div  style="cursor:pointer;width:150px; overflow:hidden; border:1px solid #000" onclick=move("'+type+'","'+data.Nodes+'","editor5");>'+subject+'</div> <br>';
			   }
			}
			document.getElementById("exploreDyna").innerHTML = explore; 
			document.getElementById("informDyna").innerHTML = inform; 
			document.getElementById("warmDyna").innerHTML = warm; 
			document.getElementById("enticeDyna").innerHTML = entice; 
			document.getElementById("convertDyna").innerHTML = convert; 
		}	
   });	
});
</script>

