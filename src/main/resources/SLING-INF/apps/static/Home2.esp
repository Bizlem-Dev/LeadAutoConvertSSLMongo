  <script>
	  var d,h,m,s,animate;
	
	  function init(){
	      d=new Date();
	  	  d.setHours(d.getHours()-5);
	  	  d.setMinutes(d.getMinutes()-30);
	      h=d.getHours();
	      m=d.getMinutes();
	      s=d.getSeconds();
	      clock();
	  };
	
	  function clock(){
	      s++;
	      if(s==60){
	          s=0;
	          m++;
	          if(m==60){
	              m=0;
	              h++;
	              if(h==24){
	                  h=0;
	              }
	          }
	      }
	      setClock('sec',s);
	      setClock('min',m);
	      setClock('hr',h);
	      animate=setTimeout(clock,1000);
	  	//console.log("animate : "+animate);
	  };
	
	  function setClock(id,val){
	      if(val<10){
	          val='0'+val;
	      }
	  	//console.log("id : "+id);
	  	//console.log("val : "+val);
	      document.getElementById(id).innerHTML=val;
	  };
	
	  window.onload=init;
  </script>
