<script>

$( document).ready(function() {
var remoteuser ='<%=request.getRemoteUser()%>';
//alert("remoteuser : "+remoteuser);
$.ajax({
    type : 'GET',
    url :'<%=request.getContextPath()%>/servlet/service/CampaignReportApi.urlViewData',
    data: {
	campaign_id : '<%=request.getParameter("campaign_id")%>',
	funnel_name : '<%=request.getParameter("funnel_name")%>',
	subfunnel_name : '<%=request.getParameter("subfunnel_name")%>',
	userid : '<%=request.getParameter("userid")%>'
 
         },
	success: function (dataa) {
        	    //alert("dataa : "+dataa);
        	    var urlClickedJsonArray = JSON.parse(dataa);
        	    var finalRow = "";
        	    var funnel_path = "";
        	    
        	    for(var i=0;i<urlClickedJsonArray.length;i++){
        	    	//var campaignclickstatisticsJsonObj=urlClickedJsonArray[i];
        	    	 finalRow=finalRow+"<tr><td><a href='<%=request.getContextPath()%>/servlet/service/CampaignReportApi.ucsUrlClickStatistics?campaign_id="+urlClickedJsonArray[i].messageid+"&url="+urlClickedJsonArray[i].url+"' target='_blank'>"+urlClickedJsonArray[i].url+"</a></td><td><a href='<%=request.getContextPath()%>/servlet/service/CampaignReportApi.ucsCampaignClickStatistics?campaign_id="+urlClickedJsonArray[i].messageid+"&url="+urlClickedJsonArray[i].url+"' target='_blank'>"+urlClickedJsonArray[i].messageid+"</a></td><td>YES</td><td>"+urlClickedJsonArray[i].firstclick+"</td><td>"+urlClickedJsonArray[i].latestclick+"</td><td>"+urlClickedJsonArray[i].clicked+"</td></tr>";					
				}
				
				
        	    //alert	("finalRow : "+finalRow);
				document.getElementById("url_details").innerHTML  =finalRow;
				document.getElementById("funnel_path").innerHTML  ='<%=request.getParameter("funnel_path")%>';
				
				
         }
    });	
});

</script>

